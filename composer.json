{
  "name": "rugalib/ruga-rugaform",
  "description": "Rugalib form plugin for jquery with middleware",
  "license": "BSD-3-Clause",
  "homepage": "https://github.com/rugalib",
  "type": "library",
  "require": {
    "php": "^7.4",
    "ext-json": "*",
    "psr/container": "^1.0",
    "psr/http-server-middleware": "^1.0",
    "laminas/laminas-servicemanager": "^3",
    "laminas/laminas-diactoros": "^2.11",
    "rugalib/ruga-log": "^1.0",
    "rugalib/ruga-std": "^1.0",
    "rugalib/ruga-db": "^1.0",
    "rugalib/ruga-asset": "^1.0",
    "rugalib/asset-jquery": "^3.6",
    "rugalib/asset-jquery-validation": "*",
    "rugalib/asset-alertifyjs": "*",
    "rugalib/asset-jquery-hotkeys": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "^9",
    "laminas/laminas-config-aggregator": "^1",
  },
  "repositories": [
    {
      "type": "composer",
      "url": "https://packages.rugalib.ch/"
    }
  ],
  "authors": [
    {
      "name": "Roland Rusch",
      "email": "roland.rusch@easy-smart.ch"
    }
  ],
  "autoload": {
    "psr-4": {
      "Ruga\\Rugaform\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Ruga\\Rugaform\\Test\\": "test/src"
    }
  },
  "extra": {
    "laminas": {
      "component": "Ruga\\Rugaform",
      "config-provider": "Ruga\\Rugaform\\ConfigProvider"
    }
  }
}